{% extends "documentation" %}

{% block content %}
	<div class="container content">
	
		<ol class="breadcrumb">
			<li><a href="/{{ contextRoot }}/documentation">Documentation</a></li>
			<li class="active">{% block guideTitle %}{% endblock %}</li>
		</ol>
		
		<h2>{{ block('guideTitle') }}</h2>
		
		<div class="row">
			<div class="col-md-9">
				{% block guideContent %}{% endblock guideContent %}
			</div>
			<div class="col-md-3">
				<ins class="adsbygoogle square-ad" data-ad-client="ca-pub-5218270417071747" data-ad-slot="6800492371"></ins>
			</div>
		</div>
		
	</div>
	
{% endblock content%}